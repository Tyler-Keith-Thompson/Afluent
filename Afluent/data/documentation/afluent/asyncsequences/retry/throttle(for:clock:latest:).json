ind":"text","text":": "},{"kind":"typeIdentifier","text":"C"},{"kind":"text","text":", "},{"kind":"externalParam","text":"latest"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"},{"text":") -> ","kind":"text"},{"text":"AsyncSequences","identifier":"doc:\/\/Afluent\/documentation\/Afluent\/AsyncSequences","preciseIdentifier":"s:7Afluent14AsyncSequencesO","kind":"typeIdentifier"},{"text":".","kind":"text"},{"text":"Throttle","identifier":"doc:\/\/Afluent\/documentation\/Afluent\/AsyncSequences\/Throttle","preciseIdentifier":"s:7Afluent14AsyncSequencesO8ThrottleV","kind":"typeIdentifier"},{"text":"<","kind":"text"},{"text":"Self","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"C","kind":"typeIdentifier"},{"text":"> ","kind":"text"},{"text":"where","kind":"keyword"},{"text":" ","kind":"text"},{"text":"C","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"Clock","preciseIdentifier":"s:12_Concurrency5ClockP","kind":"typeIdentifier"}]}],"kind":"declarations"},{"parameters":[{"name":"interval","content":[{"type":"paragraph","inlineContent":[{"text":"The length of time to wait between emissions.","type":"text"}]}]},{"name":"clock","content":[{"type":"paragraph","inlineContent":[{"text":"The clock used to measure time intervals and perform sleeps.","type":"text"}]}]},{"name":"latest","content":[{"type":"paragraph","inlineContent":[{"text":"A Boolean flag determining which element to emit during each interval:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"false"},{"text":" emits the first element seen in the interval.","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"true","type":"codeVoice"},{"text":" emits the latest element seen during the interval.","type":"text"}],"type":"paragraph"}]}]}]}],"kind":"parameters"},{"content":[{"text":"Return Value","level":2,"type":"heading","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"text":"An async sequence that emits throttled elements from the upstream sequence.","type":"text"}]}],"kind":"content"},{"content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"This operator suppresses elements from the upstream async sequence during each interval, emitting only one element per interval:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"text","text":"If "},{"type":"codeVoice","code":"latest"},{"type":"text","text":" is "},{"type":"codeVoice","code":"false"},{"type":"text","text":", it emits the first element received immediately and then ignores subsequent elements until the interval completes."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"If ","type":"text"},{"type":"codeVoice","code":"latest"},{"text":" is ","type":"text"},{"type":"codeVoice","code":"true"},{"text":", it emits the first element immediately, then during the interval it tracks element updates, emitting only the most recent one at the intervalâ€™s end.","type":"text"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Elements arriving during an interval beyond the emitted one are suppressed, allowing controlled pacing of element emission."}]},{"text":"Example","type":"heading","level":2,"anchor":"Example"},{"type":"paragraph","inlineContent":[{"text":"The following example creates an ","type":"text"},{"type":"codeVoice","code":"AsyncStream"},{"text":" that emits the current date every second,","type":"text"},{"text":" ","type":"text"},{"text":"then applies a throttle with a 3-second interval, showing both ","type":"text"},{"type":"codeVoice","code":"latest: false"},{"text":" and ","type":"text"},{"type":"codeVoice","code":"latest: true"},{"text":".","type":"text"}]},{"type":"codeListing","code":["import Foundation","import _Concurrency","","let stream = AsyncStream<Date> { continuation in","    Task {","        while true {","            continuation.yield(Date())","            try await Task.sleep(nanoseconds: 1_000_000_000) \/\/ 1 second","        }","    }","}","","\/\/ Throttle with latest: false (emit the first element immediately, then suppress others during the interval)","Task {","    print(\"Throttle with latest: false\")","    for await date in stream.throttle(for: .seconds(3), clock: ContinuousClock(), latest: false) {","        print(\"Emitted (first): \\(date)\")","    }","}","","\/\/ Throttle with latest: true (emit the first element immediately, then emit the latest element at interval end)","Task {","    print(\"Throttle with latest: true\")","    for await date in stream.throttle(for: .seconds(3), clock: ContinuousClock(), latest: true) {","        print(\"Emitted (latest): \\(date)\")","    }","}","","\/*","Sample output for latest: false:","Throttle with latest: false","Emitted (first): 2025-07-04 12:00:00 +0000","Emitted (first): 2025-07-04 12:00:03 +0000","Emitted (first): 2025-07-04 12:00:06 +0000","","Sample output for latest: true:","Throttle with latest: true","Emitted (latest): 2025-07-04 12:00:00 +0000","Emitted (latest): 2025-07-04 12:00:02 +0000","Emitted (latest): 2025-07-04 12:00:05 +0000","*\/"],"syntax":"swift"}],"kind":"content"}],"abstract":[{"type":"text","text":"Allows only one element to pass through during each specified interval, emitting either the first or the latest element seen in that period."}],"sections":[],"references":{"doc://Afluent/documentation/Afluent/AsyncSequences/Retry":{"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"Retry","kind":"identifier"}],"role":"symbol","title":"AsyncSequences.Retry","navigatorTitle":[{"text":"Retry","kind":"identifier"}],"abstract":[],"url":"\/documentation\/afluent\/asyncsequences\/retry","type":"topic","kind":"symbol","identifier":"doc:\/\/Afluent\/documentation\/Afluent\/AsyncSequences\/Retry"},"doc://Afluent/documentation/Afluent/AsyncSeq